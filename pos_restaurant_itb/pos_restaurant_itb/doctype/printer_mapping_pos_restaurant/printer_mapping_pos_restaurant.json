{
    "doctype": "DocType",
    "name": "Printer Mapping POS Restaurant",
    "module": "pos_restaurant_itb",
    "custom": 0,
    "istable": 1,
    "editable_grid": 1,
    "fields": [
      {
        "fieldname": "printer_name",
        "label": "Printer Name",
        "fieldtype": "Data",
        "reqd": 1
      },
      {
        "fieldname": "printer_type",
        "label": "Printer Type",
        "fieldtype": "Select",
        "options": "Thermal\nBluetooth",
        "default": "Thermal"
      },
      {
        "fieldname": "printer_ip",
        "label": "Printer IP / Address",
        "fieldtype": "Data",
        "depends_on": "eval:doc.printer_type === 'Thermal'",
        "description": "Hanya untuk printer Thermal (gunakan IP atau hostname)"
      },
      {
        "fieldname": "bluetooth_identifier",
        "label": "Bluetooth Identifier",
        "fieldtype": "Data",
        "depends_on": "eval:doc.printer_type === 'Bluetooth'",
        "description": "MAC Address atau Nama Device Bluetooth"
      },
      {
        "fieldname": "print_format",
        "label": "Print Format",
        "fieldtype": "Link",
        "options": "Print Format"
      }
    ]
  }
  